// add_numbers.cpp
#include <iostream>
using namespace std;

int main() {
    double num1, num2, sum;
    cout << "Enter first number: ";
    cin >> num1;
    cout << "Enter second number: ";
    cin >> num2;
    sub = num1 - num2;
    cout << "The sub of " << num1 << " and " << num2 << " is " << sub << endl;
    return 0;
}
